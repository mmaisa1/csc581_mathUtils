pipeline {
    agent any 
    stages {
        stage('Build') { 
            steps {
                bat 'python -m py_compile MathUtils.py' 
                stash(name: 'compiled-results', includes: '*.py*') 
            }	   	
        }
	stage('Test') {
            steps {
                bat 'py.test --junit-xml test-reports/results.xml test_MathUtils.py'
            }
            post {
                always {
                    junit 'test-reports/results.xml'
                }
            }
        }
    }
}
